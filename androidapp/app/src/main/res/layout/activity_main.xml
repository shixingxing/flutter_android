<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/preCachedFlutter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cached Flutter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/routerName"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:hint="router name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/preCachedFlutter" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/routerFlutter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Router Flutter"
        app:layout_constraintStart_toEndOf="@id/routerName"
        app:layout_constraintTop_toTopOf="@id/routerName" />

</androidx.constraintlayout.widget.ConstraintLayout>